{
  "name": "caliopen.web",
  "version": "0.1.0",
  "description": "CaliOpen Web Server using NodeJS/Express",
  "repository": {
    "type": "git",
    "url": "git@github.com:CaliOpen/caliopen.web.git"
  },
  "author": "Caliopen",
  "private": false,
  "scripts": {
    "start": "DEBUG=caliopen.web:* NODE_ENV=development node ./bin/server",
    "start:watch": "DEBUG=caliopen.web:* NODE_ENV=development nodemon ./bin/server",
    "start:production": "NODE_ENV=production pm2 -i -1 start ./bin/server --name caliopen-web",
    "lint": "eslint ./app",
    "test": "mocha --recursive",
    "release": "node ./bin/build",
    "prerelease": "npm test",
    "check-deps": "check-deps -d"
  },
  "engines": {
    "node": ">=0.10.40"
  },
  "dependencies": {
    "body-parser": "~1.15.0",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "http-proxy": "^1.11.2",
    "iron": "^2.1.3",
    "morgan": "~1.7.0",
    "nunjucks": "^1.3.4",
    "object-assign": "^4.0.1",
    "object-keys": "~1.0.7",
    "pm2": "^0.14.7",
    "yamljs": "^0.2.4"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "check-deps": "^1.4.0",
    "eslint": "^1.7.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.7.1"
  }
}
